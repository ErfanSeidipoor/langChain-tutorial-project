  "description": "This project serves as a practical guide to LangChain's streaming capabilities, enabling you to interact with large language models (LLMs) in a continuous, real-time fashion.",
  "main": "index.js",
  "scripts": {
    "index": "node -r ts-node/register --env-file=.env src/1-index.ts",
    "chain": "node -r ts-node/register --env-file=.env src/2-chain.ts",
    "json-output": "node -r ts-node/register --env-file=.env src/3-json-output.ts",
    "json-output-extraction": "node -r ts-node/register --env-file=.env src/4-json-output-extraction.ts",
    "non-streaming-components": "node -r ts-node/register --env-file=.env src/5-non-streaming-components.ts",
    "runnable-passthrough": "node -r ts-node/register --env-file=.env src/6-runnable-passthrough",
    "stream-events": "node -r ts-node/register --env-file=.env src/7-stream-events",
    "stream-events-json-parser": "node -r ts-node/register --env-file=.env src/8-stream-events-json-parser"
  },
